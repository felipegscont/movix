# Configurações do ambiente
APP_ENV=development
APP_DEBUG=true

# Configurações da NFe
NFE_AMBIENTE=2
# 1 = Produção, 2 = Homologação

# Certificado Digital
CERT_PATH=/var/www/html/certificates/certificado.pfx
CERT_PASSWORD=senha_do_certificado

# Dados da emitente
EMPRESA_CNPJ=00000000000000
EMPRESA_RAZAO_SOCIAL=Emitente Exemplo LTDA
EMPRESA_NOME_FANTASIA=Emitente Exemplo
EMPRESA_INSCRICAO_ESTADUAL=000000000
EMPRESA_INSCRICAO_MUNICIPAL=000000
EMPRESA_CNAE=0000000
EMPRESA_REGIME_TRIBUTARIO=1
# 1=Simples Nacional, 2=Simples Nacional - excesso, 3=Regime Normal

# Endereço da emitente
EMPRESA_LOGRADOURO=Rua Exemplo, 123
EMPRESA_NUMERO=123
EMPRESA_BAIRRO=Centro
EMPRESA_CEP=00000000
EMPRESA_MUNICIPIO=São Paulo
EMPRESA_UF=SP
EMPRESA_TELEFONE=1199999999
EMPRESA_EMAIL=contato@emitente.com.br

# Configurações de log
LOG_LEVEL=debug
LOG_PATH=/var/www/html/logs

# Configurações de segurança
API_KEY=sua_api_key_secreta_aqui

# Configurações de rate limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=3600
